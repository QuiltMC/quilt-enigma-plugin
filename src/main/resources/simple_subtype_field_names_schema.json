{
    "$schema": "https://json-schema.org/draft-07/schema",
    "title": "Simple Subtype Field Names Registry",
    "description": "Definitions for the proposed names of \"simple\" subtypes",
    "type": "object",
    "patternProperties": {
        "^(?:\\b[_a-zA-Z]|\\B\\$)[_$a-zA-Z0-9]*$": {
            "description": "Suffix of subtypes",
            "type": "string",
            "minLength": 1,
            "pattern": "[_$a-zA-Z0-9]+"
        }
    }
}
